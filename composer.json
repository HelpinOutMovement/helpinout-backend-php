        "yiisoft/yii2-httpclient": "~2.0.5",
        "yiisoft/yii2-imagine": "~2.1.0",
        "mdmsoft/yii2-admin": "~2.0",
        "2amigos/yii2-tinymce-widget": "~1.1",
        "2amigos/yii2-google-maps-library": "*",
        "dektrium/yii2-user": "~0.9.12",
        "himiklab/yii2-recaptcha-widget": "~1.1.1",
        "himiklab/yii2-colorbox-widget": "~1.0.3",
        "miloschuman/yii2-highcharts-widget": "~6.0",
        "kop/yii2-scroll-pager": "dev-master",
        "kop/yii2-conditional-validator": "dev-master",
        "nex/yii2-chosen": "*",
        "philippfrenzel/yii2fullcalendar": "~3.8.0",
        "kartik-v/yii2-krajee-base": "dev-master",
        "kartik-v/yii2-widgets": "dev-master",
        "kartik-v/yii2-grid": "dev-master",
        "kartik-v/yii2-dynagrid": "dev-master",
        "kartik-v/yii2-sortable": "dev-master",
        "kartik-v/yii2-builder": "dev-master",
        "kartik-v/yii2-detail-view": "dev-master",
        "kartik-v/yii2-label-inplace": "dev-master",
        "kartik-v/yii2-helpers": "dev-master",
        "kartik-v/yii2-icons": "dev-master",
        "kartik-v/yii2-dropdown-x": "dev-master",
        "kartik-v/yii2-nav-x": "@dev",
        "kartik-v/yii2-slider": "dev-master",
        "kartik-v/yii2-password": "dev-master",
        "kartik-v/yii2-sortable-input": "dev-master",
        "kartik-v/yii2-checkbox-x": "dev-master",
        "kartik-v/yii2-markdown": "dev-master",
        "kartik-v/yii2-date-range": "dev-master",
        "kartik-v/yii2-datecontrol": "dev-master",
        "kartik-v/yii2-money": "dev-master",
        "kartik-v/yii2-tabs-x": "dev-master",
        "kartik-v/yii2-editable": "dev-master",
        "kartik-v/yii2-popover-x": "dev-master",
        "kartik-v/yii2-export": "dev-master",
        "warrence/yii2-kartikgii": "dev-master",
        "kartik-v/bootstrap-fileinput": "dev-master",
        "kartik-v/yii2-context-menu": "dev-master",
        "kartik-v/bootstrap-tabs-x": "dev-master",
        "kartik-v/strength-meter": "dev-master",
        "kartik-v/bootstrap-star-rating": "dev-master",
        "kartik-v/bootstrap-checkbox-x": "dev-master",
        "kartik-v/yii2-field-range": "dev-master",
        "kartik-v/yii2-tree-manager": "dev-master",
        "kartik-v/yii2-mpdf": "dev-master",
        "mpdf/mpdf": "*",
        "robregonm/yii2-pdf": "*",
        "dmstr/yii2-adminlte-asset": "2.4.4",
        "ofat/yii2-yashop-ses": "*",
        "daniel-zahariev/php-aws-ses": "~0.9.1",
        "bedezign/yii2-audit": "^1.0",
        "phpoffice/phpword": "dev-master",
        "phtamas/yii2-imageprocessor": "0.5.*",
        "2amigos/yii2-ckeditor-widget": "~2.1",
        "unclead/yii2-multiple-input": "~2.0",
        "wbraganca/yii2-dynamicform": "*",
        "zxbodya/yii2-gallery-manager": "*@dev",
        "zxbodya/yii2-tinymce": "*@dev",
        "nirvana-msu/yii2-showloading": "1.0.*@dev",
        "kalyabin/yii2-select-google-map-location": "*",
        "newerton/yii2-fancybox": "dev-master",
        "cornernote/yii2-dashboard": "*",
        "slavkovrn/yii2-lightbox": "^1.0",
        "kartik-v/yii2-dialog": "@dev",
        "ibrarturi/yii2-latlng-finder": "dev-master",
        "2amigos/qrcode-library": "~1.1",
        "bluezed/yii2-scroll-top": "*",
        "kartik-v/yii2-social": "@dev",
        "phpoffice/phpspreadsheet": "*",
        "opensooq/yii2-firebase-notifications": "dev-master"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.1.0",
        "yiisoft/yii2-gii": "~2.1.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/codeception": "^4.0",
        "codeception/verify": "~0.5.0 || ~1.1.0",
        "codeception/specify": "~0.4.6",
        "symfony/browser-kit": ">=2.7 <=4.2.4",
        "codeception/module-filesystem": "^1.0.0",
        "codeception/module-yii2": "^1.0.0",
        "codeception/module-asserts": "^1.0.0"
    },
    "config": {
        "process-timeout": 1800,
        "fxp-asset": {
            "enabled": false
        }
    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
